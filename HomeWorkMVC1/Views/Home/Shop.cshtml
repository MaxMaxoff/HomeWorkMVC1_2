@model HomeWorkMVC1.Models.Product.CatalogViewModel
@{
    ViewData["Title"] = "Shop";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@await Html.PartialAsync("Partial/_BreadCrums")
@await Html.PartialAsync("Partial/_Advertisement")

<section>
    <div class="row">
        <div class="col-sm-3">
            @await Html.PartialAsync("Partial/_LeftSideBar")
        </div>
        <div class="col-sm-9 padding-right">
            @await Html.PartialAsync("Partial/_ProductItems", Model.Products)
            @*@await Html.PartialAsync("Partial/_Features")*@
        </div>
    </div>
</section>